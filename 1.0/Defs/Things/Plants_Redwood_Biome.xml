<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <ThingDef ParentName="TreeBase">
    <defName>TKKN_PlantTreeRedwood</defName>
    <label>redwood</label>
    <description>A huge, very slow growing tree. Its roots are shallow, and when it gets too large, it topples over. \n\n Yields red wood, which is valuable and rare.</description>
    <graphicData>
      <texPath>TKKN_NPS/Things/Plant/TreeRedwoods</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.55, 0.2, 0.2)</volume>
        <offset>(0,0,-0.25)</offset>
      </shadowData>
    </graphicData>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <statBases>
      <Beauty>15</Beauty>
    </statBases>
    <blockLight>true</blockLight>
    <plant>
      <sowTags />
      <fertilityMin>0.8</fertilityMin>
      <harvestedThingDef>RedWoodLog</harvestedThingDef>
      <fertilitySensitivity>0.8</fertilitySensitivity>
      <growDays>300</growDays>
      <harvestWork>3000</harvestWork>
      <harvestYield>225</harvestYield>
      <wildClusterWeight>8</wildClusterWeight>
      <wildClusterRadius>10</wildClusterRadius>
      <visualSizeRange>
        <min>8</min>
        <max>12</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  <ThingDef ParentName="TreeBase">
    <defName>TKKN_OldGrowth_PlantTreeOak</defName>
    <label>old growth oak tree</label>
    <description>A strong hardwood tree. Slow-going, but its wood is very strong.</description>
    <graphicData>
      <texPath>Things/Plant/TreeOak</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>2</Beauty>
    </statBases>
    <plant>
      <sowTags />
      <fertilityMin>0.4</fertilityMin>
      <growDays>60</growDays>
      <leaflessGraphicPath>Things/Plant/TreeOak_Leafless</leaflessGraphicPath>
      <harvestWork>1000</harvestWork>
      <harvestYield>75</harvestYield>
      <wildClusterWeight>32</wildClusterWeight>
      <wildClusterRadius>5</wildClusterRadius>
      <visualSizeRange>
        <min>2</min>
        <max>4</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  <ThingDef ParentName="TreeBase">
    <defName>TKKN_OldGrowth_PlantTreeHazelnut</defName>
    <label>old growth hazelnut tree</label>
    <description>A multi-stemmed woodland shrub with soft, saw-toothed, velvety leaves.</description>
    <graphicData>
      <texPath>TKKN_NPS/Things/Plant/TreeOldGrowthHazel</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>2</Beauty>
    </statBases>
    <plant>
      <sowTags />
      <leaflessGraphicPath>TKKN_NPS/Things/Plant/VarientLeafless/TreeOldGrowthHazel_Leafless</leaflessGraphicPath>
      <growDays>60</growDays>
      <harvestWork>1000</harvestWork>
      <harvestYield>75</harvestYield>
      <wildClusterWeight>30</wildClusterWeight>
      <wildClusterRadius>5</wildClusterRadius>
      <visualSizeRange>
        <min>2</min>
        <max>4</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  <ThingDef ParentName="TreeBase">
    <defName>TKKN_PlantTreeHazelnut</defName>
    <label>hazelnut tree</label>
    <description>A multi-stemmed woodland shrub with soft, saw-toothed, velvety leaves.</description>
    <graphicData>
      <texPath>TKKN_NPS/Things/Plant/TreeHazel</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>2</Beauty>
    </statBases>
    <plant>
      <leaflessGraphicPath>TKKN_NPS/Things/Plant/VarientLeafless/TreeHazel_Leafless</leaflessGraphicPath>
      <growDays>60</growDays>
      <harvestWork>1000</harvestWork>
      <harvestYield>75</harvestYield>
      <wildClusterWeight>30</wildClusterWeight>
      <wildClusterRadius>5</wildClusterRadius>
    </plant>
  </ThingDef>
  <ThingDef ParentName="TreeBase">
    <defName>TKKN_PlantTreeRedwoodDead</defName>
    <label>redwood</label>
    <description>A huge, very slow growing tree. This one has toppled over due to its weight. \n\n Yeilds red wood, which is valuable and rare.</description>
    <graphicData>
      <texPath>TKKN_NPS/Things/Plant/TreeRedwoodDead</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.55, 0.2, 0.2)</volume>
        <offset>(0,0,-0.25)</offset>
      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>15</Beauty>
    </statBases>
    <plant>
      <sowTags />
      <fertilityMin>0.7</fertilityMin>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <growDays>0</growDays>
      <harvestedThingDef>RedWoodLog</harvestedThingDef>
      <harvestWork>4000</harvestWork>
      <harvestYield>250</harvestYield>
      <wildClusterWeight>1</wildClusterWeight>
      <wildClusterRadius>3</wildClusterRadius>
      <visualSizeRange>
        <min>7</min>
        <max>9</max>
      </visualSizeRange>
      <topWindExposure>0</topWindExposure>
    </plant>
  </ThingDef>
  <ThingDef ParentName="PlantBase">
    <defName>TKKN_PlantFern</defName>
    <label>fern</label>
    <description>A soft, leafy plant that reproduces using spores.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.25</Nutrition>
    </statBases>
    <pathCost>18</pathCost>
    <graphicData>
      <texPath>TKKN_NPS/Things/Plant/Ferns</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.6</hideAtSnowDepth>
    <ingestible />
    <plant>
      <fertilityMin>0.5</fertilityMin>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <growDays>3</growDays>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestWork>60</harvestWork>
      <maxMeshCount>9</maxMeshCount>
      <visualSizeRange>
        <min>0.7</min>
        <max>0.85</max>
      </visualSizeRange>
      <topWindExposure>0.4</topWindExposure>
    </plant>
  </ThingDef>
