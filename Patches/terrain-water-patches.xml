    <xpath>/Defs/TerrainDef[defName = "WaterMovingShallow"]</xpath>
    <value>
      <fertility>0.001</fertility>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TerrainDef[defName = "WaterShallow"]</xpath>
