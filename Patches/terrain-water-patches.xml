    <success>Always</success>
    <match Class="PatchOperationRemove">
      <xpath>/Defs/TerrainDef[@Name = "WaterDeepBase"]/passability</xpath>
    </match>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TerrainDef[@Name = "WaterBase"]/tags</xpath>
    <value>
      <li>TKKN_Wet</li>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TerrainDef[@Name = "WaterDeepBase"]</xpath>
    <value>
      <tags />
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TerrainDef[@Name = "WaterDeepBase"]/tags</xpath>
    <value>
      <li>TKKN_Swim</li>
    </value>
  </Operation>
  <Operation Class="PatchOperationInsert">
    <xpath>/Defs/TerrainDef[defName = "Marsh"]</xpath>
    <value>
      <tags>
        <li>TKKN_Wet</li>
      </tags>
    </value>
  </Operation>
